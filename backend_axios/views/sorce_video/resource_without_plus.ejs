</head>
<body>

    <!-- nab bar -->
    <div class="nav_bar">
        <div class="logo"></div>
    </div>

<!-- main continer -->
<div class="gra_nav"></div>


    <div class="main_continer">

        <div class="unit_continer">
            <a href="<%=`${our_domain}${dept}/${sub}/1`%>"><button class="go_back left_bar"><span class="text">Go back</span></button></a>
            <a href="<%=`/${dept}/${sub}/${unit}/${topic}/video_res` %>"><div class="left_bar"><center>Video</csenter></div></a>
            <a href="<%=`/${dept}/${sub}/${unit}/${topic}/doc_res` %>"><div class="left_bar"><center>Documents</center></div></a>
            <a href="<%=`/${dept}/${sub}/${unit}/${topic}/link_res` %>"><div class="left_bar"><center>Links</center></div></a>
        </div>

        <div class="main_topic_continer">
            <div class="topic_head_continer">
                <h1><span>Video:</span></h1>
            </div>
            <div class="main_video_content">
                <%if (locals.data){%>
                <%for(let i=0;i<data.length;i++){%>
                <div class="video_continer">
                    <span class="text1">

                    <div class="video_section">
                        <iframe src="<%=`https://www.youtube.com/embed/${data[i].video}` %>">
                        </iframe>
                    </div>
                    <div class="video_description">
                        <div class="doc_title"><h2>Video Title:</h2> 
                            <h3> &ensp;<%= data[i].video_title%></h3>
                        </div>
                        <div class="doc_des"> <h2>Video Description:</h2><p> &ensp;<%=data[i].video_desc %></p></div>
                    </div>
                </div>
            </span>
                <%}}%>

                
                
            </div>
        </div>


    </div>

   

    </div>
